.container{
  display: flex;
  position: relative;
  align-items: center;
  margin-left: 20rpx;
}
.input-bar{
  flex: 1;
  height: 68rpx;
  padding-left: 60rpx;
  font-size: 28rpx;
  border-radius: 50px;
  background-color: #f5f5f5;
}
.search{
  background-color: #d43c33;
  color: #fff;
  margin-left: 20rpx;
  font-size: 18rpx;
}

.search-icon{
  position: absolute;
  top: 50%;
  transform: translate(16rpx,-50%);
  color: #9f9f9f;
}